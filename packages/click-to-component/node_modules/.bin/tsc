#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/duanjipeng/Desktop/ki-click-to-component/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/Users/duanjipeng/Desktop/ki-click-to-component/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/Users/duanjipeng/Desktop/ki-click-to-component/node_modules/.pnpm/typescript@4.6.3/node_modules:/Users/duanjipeng/Desktop/ki-click-to-component/node_modules/.pnpm/node_modules:/Users/duanjipeng/Desktop/ki-click-to-component/node_modules:/Users/duanjipeng/Desktop/node_modules:/Users/duanjipeng/node_modules:/Users/node_modules:/node_modules:/Users/duanjipeng/Desktop/ki-click-to-component/packages/click-to-component/node_modules:/Users/duanjipeng/Desktop/ki-click-to-component/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/duanjipeng/Desktop/ki-click-to-component/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/Users/duanjipeng/Desktop/ki-click-to-component/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/Users/duanjipeng/Desktop/ki-click-to-component/node_modules/.pnpm/typescript@4.6.3/node_modules:/Users/duanjipeng/Desktop/ki-click-to-component/node_modules/.pnpm/node_modules:/Users/duanjipeng/Desktop/ki-click-to-component/node_modules:/Users/duanjipeng/Desktop/node_modules:/Users/duanjipeng/node_modules:/Users/node_modules:/node_modules:/Users/duanjipeng/Desktop/ki-click-to-component/packages/click-to-component/node_modules:/Users/duanjipeng/Desktop/ki-click-to-component/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/tsc" "$@"
fi
